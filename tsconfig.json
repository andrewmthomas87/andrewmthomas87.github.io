{
	"compilerOptions": {
		"baseUrl": "src",
		"experimentalDecorators": true,
		"jsx": "react",
		"noImplicitAny": true,
		"removeComments": true,
		"strictNullChecks": true,
		"target": "es5"
	},
	"exclude": [
		"node_modules"
	],
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx",
		"typings/*.d.ts"
	]
}
